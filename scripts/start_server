# !/bin/bash

cd /home/ubuntu/build/forour-backend
aws ssm get-parameters --name "LEVEL" "PRODUCT_DB_URL" > env.json
jq -jr '.Parameters[]|.Name, "=", .Value, "\n"' < env.json >> .env
pip install poetry
poetry install
gunicorn -k uvicorn.workers.UvicornWorker --access-logfile ./gunicorn-access.log src.main:app --bind 0.0.0.0:80 --workers 2 --daemon
